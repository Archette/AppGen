{
    "name": "archette/appgen",
    "description": "Model layer (Doctrine ORM) generator to speed up development",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Rixafy",
            "email": "rixafy@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "nette/neon": "^3.0.0",
        "nette/schema": "^1.0.0",
        "nette/php-generator": "^3.5",
        "symfony/console": "^5.0.0",
        "nette/finder": "^3.0.0"
    },
    "bin": [
        "bin/appgen",
        "bin/appgen.phar"
    ],
    "autoload": {
        "psr-4": {
            "Archette\\AppGen\\": "src"
        }
    },
    "config": {
        "platform": {
            "php": "8.0"
        }
    }
}
